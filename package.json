		"@jest/globals": "^29.7.0",
		"@types/fs-extra": "^11.0.4",
		"@types/jest": "^29.5.12",
		"@types/lodash": "^4.17.6",
		"@types/mime-types": "^2.1.4",
		"@types/uuid": "^9.0.8",
